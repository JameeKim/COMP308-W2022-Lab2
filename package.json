{
  "name": "dohyunkim-comp308lab2-ex1",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "common",
    "client",
    "server"
  ],
  "scripts": {
    "start": "npm run start -w server",
    "dev-client": "npm run start -w client",
    "dev-server": "npm run dev -w server",
